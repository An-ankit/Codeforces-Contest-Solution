#include <bits/stdc++.h>
#define ll long long
 
using namespace std;
void solution(){
    int a,b,c,d;
    cin>>a>>b>>c>>d;
    if(a==b && b==c && c==d && a==0)cout<<0<<endl;
    else if(a==b && b==c && c==d && a==1)cout<<2<<endl;
    else cout<<1<<endl;
    
    
    
}
 
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    
    int t;
    cin>>t;
    while(t--){
        solution();
    }
 
    return 0;
}
